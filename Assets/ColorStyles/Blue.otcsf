/*    OpenTwin Notes                                                                     */
/*                                                                                       */
/*    Splitter Handle:                                                                   */
/*       When changing the width the qt-ads (third party) library must be adjusted       */
/*       (see notes file) and recompiled.                                                */
/*                                                                                       */
/* This file used the "QDarkStyleSheet/qdarkstyle/qss/_style.scss" as a template.        */
/*                                                                                       */

/* Reset */

* {
    padding: 0px;
    margin: 0px;
    border: 0px;
    border-style: none;
    border-image: none;
    outline: 0;
}

QToolBar * {
    margin: 0px;
    padding: 0px;
}

/* ##################################################################################### */

/* QWidget */

QWidget {
    background-color: rgba(0,64,128,255);
    border: none;
    padding: 0px;
    color: rgba(255,255,255,255);
    selection-background-color: rgba(0,255,0,255);
    selection-color: rgba(0,0,0,255);
}

QWidget:disabled {
    background-color: rgba(0,44,108,255);
    color: rgba(127,127,127,255);
    selection-background-color: rgba(0,44,108,255);
    selection-color: rgba(127,127,127,255);
}

QWidget::item {
    background-color: rgba(0,64,128,255);
}

QWidget::item:selected {
    background-color: rgba(0,255,0,255);
    color: rgba(0,0,0,255);
}

QWidget::item:hover {
    background-color: rgba(0,215,255,255);
}

/* ##################################################################################### */

/* QMainWindow */

QMainWindow {
    background-color: rgba(0,64,128,255);
}

QMainWindow::separator {
    background-color: rgba(0,90,156,255);
    spacing: 0px;
    padding: 2px;
}

QMainWindow::separator:hover:!pressed {
    background-color: rgba(0,215,255,255);
}

QMainWindow::separator:pressed {
    background-color: rgba(0,255,0,255);
}

QMainWindow::separator:horizontal {
    width: 2;
    margin-top: 2px;
    margin-bottom: 2px;
}

QMainWindow::separator:vertical {
    height: 2;
    margin-left: 2px;
    margin-right: 2px;
}

/* ##################################################################################### */

/* QToolTip */

QToolTip {
    background-color: rgba(0,64,128,255);
    color: rgba(255,255,255,255);
    border: 1px solid rgba(0,100,180,255);
    /* Remove padding, for fix combo box tooltip */
    padding: 0px;
}

/* ##################################################################################### */

/* QStatusBar */

QStatusBar {
    background: rgba(0,64,128,255);
    border-top: 1px solid rgba(0,100,180,255);
}

QStatusBar::item {
    border: 1px solid transparent;
}

QStatusBar QLabel {
    background: transparent;
}

/* ##################################################################################### */

/* QCheckBox */

QCheckBox {
    background-color: transparent;
    color: rgba(255,255,255,255);
    spacing: 4px;
    outline: none;
    padding-top: 4px;
    padding-bottom: 4px;
}
    
QCheckBox:focus {
    border: none;
}

QCheckBox:disabled {
    background-color: transparent;
    color: rgba(127,127,127,255);
}

QCheckBox::indicator {
    margin-left: 2px;
    height: 14px;
    width: 14px;
}
 
QCheckBox::indicator:unchecked {
    image: url('%root%/icons/checkbox_unchecked.png');
}

QCheckBox::indicator:unchecked:hover,
QCheckBox::indicator:unchecked:focus {
    border: none;
    image: url('%root%/icons/checkbox_unchecked_focus.png');
}

QCheckBox::indicator:unchecked:pressed {
    border: none;
    image: url('%root%/icons/checkbox_unchecked_pressed.png');
}

QCheckBox::indicator:unchecked:disabled {
    image: url('%root%/icons/checkbox_unchecked_disabled.png');
}

QCheckBox::indicator:checked {
    image: url('%root%/icons/checkbox_checked.png');
}

QCheckBox::indicator:checked:hover,
QCheckBox::indicator:checked:focus {
    border: none;
    image: url('%root%/icons/checkbox_checked_focus.png');
}

QCheckBox::indicator:checked:pressed {
    border: none;
    image: url('%root%/icons/checkbox_checked_pressed.png');
}

QCheckBox::indicator:checked:disabled {
    image: url('%root%/icons/checkbox_checked_disabled.png');
}

QCheckBox::indicator:indeterminate {
    image: url('%root%/icons/checkbox_indeterminate.png');
}

QCheckBox::indicator:indeterminate:disabled {
    image: url('%root%/icons/checkbox_indeterminate_disabled.png');
}

QCheckBox::indicator:indeterminate:focus,
QCheckBox::indicator:indeterminate:hover {
    image: url('%root%/icons/checkbox_indeterminate_focus.png');
}

QCheckBox::indicator:indeterminate:pressed {
    image: url('%root%/icons/checkbox_indeterminate_pressed.png');
}

/* ##################################################################################### */

/* QGroupBox */

QGroupBox {
    font-weight: bold;
    border: 1px solid rgba(0,100,180,255);
    border-radius: 4;
    padding: 2px;
    margin-top: 6px;
    margin-bottom: 4px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    left: 4px;
    padding-left: 2px;
    padding-right: 4px;
    padding-top: -4px;
}

QGroupBox::indicator {
    margin-left: 2px;
    margin-top: 2px;
    padding: 0;
    height: 14px;
    width: 14px;
}

QGroupBox::indicator:unchecked {
    border: none;
    image: url('%root%/icons/checkbox_unchecked.png');
}

QGroupBox::indicator:unchecked:hover,
QGroupBox::indicator:unchecked:focus,
QGroupBox::indicator:unchecked:pressed {
    border: none;
    image: url('%root%/icons/checkbox_unchecked_focus.png');
}

QGroupBox::indicator:unchecked:disabled {
    image: url('%root%/icons/checkbox_unchecked_disabled.png');
}

QGroupBox::indicator:checked {
    border: none;
    image: url('%root%/icons/checkbox_checked.png');
}

QGroupBox::indicator:checked:hover,
QGroupBox::indicator:checked:focus,
QGroupBox::indicator:checked:pressed {
    border: none;
    image: url('%root%/icons/checkbox_checked_focus.png');
}

QGroupBox::indicator:checked:disabled {
    image: url('%root%/icons/checkbox_checked_disabled.png');
}

/* ##################################################################################### */

/* QRadioButton */

QRadioButton {
    background-color: rgba(0,64,128,255);
    color: rgba(255,255,255,255);
    spacing: 4px;
    padding-top: 4px;
    padding-bottom: 4px;
    border: none;
    outline: none;
}

QRadioButton:focus {
    border: none;
}

QRadioButton:disabled {
    background-color: rgba(0,64,128,255);
    color: rgba(127,127,127,255);
    border: none;
    outline: none;
}

QRadioButton QWidget {
    background-color: rgba(0,64,128,255);
    color: rgba(255,255,255,255);
    spacing: 0px;
    padding: 0px;
    outline: none;
    border: none;
}

QRadioButton::indicator {
    border: none;
    outline: none;
    margin-left: 2px;
    height: 14px;
    width: 14px;
}

QRadioButton::indicator:unchecked {
    image: url('%root%/icons/radio_unchecked.png');
}

QRadioButton::indicator:unchecked:hover,
QRadioButton::indicator:unchecked:focus,
QRadioButton::indicator:unchecked:pressed {
    border: none;
    outline: none;
    image: url('%root%/icons/radio_unchecked_focus.png');
}

QRadioButton::indicator:disabled {
    image: url('%root%/icons/radio_unchecked_disabled.png');
}
        
QRadioButton::indicator:disabled:checked {
    border: none;
    outline: none;
    image: url('%root%/icons/radio_checked.png');
}

QRadioButton::indicator:disabled:checked:hover,
QRadioButton::indicator:disabled:checked:focus,
QRadioButton::indicator:disabled:checked:pressed {
    border: none;
    outline: none;
    image: url('%root%/icons/radio_checked_focus.png');
}

QRadioButton::indicator:disabled:checked:disabled {
    outline: none;
    image: url('%root%/icons/radio_checked_disabled.png');
}

/* ##################################################################################### */

/* QMenuBar */

QMenuBar {
    background-color: rgba(0,64,128,255);
    padding: 2px;
    border-bottom: 1px solid rgba(0,100,180,255);
    margin-bottom: 2px;
    color: rgba(255,255,255,255);
    selection-background-color: rgba(0,255,0,255);
    selection-color: rgba(0,0,0,255);
}

QMenuBar:focus {
    border-color: rgba(0,215,255,255);
}

QMenuBar::item {
    background: transparent;
    padding: 4px;
}

QMenuBar::item:selected {
    padding: 4px;
    border: 0px solid rgba(0,100,180,255);
    background-color: rgba(0,215,255,255);
    color: rgba(0,0,0,255);
}

QMenuBar::item:pressed {
    padding: 4px;
    border: 0px solid rgba(0,64,128,255);
    background-color: rgba(0,255,0,255);
    color: rgba(0,0,0,255);
    margin-bottom: 0px;
    padding-bottom: 0px;
}

/* ##################################################################################### */

/* QMenu */

QMenu {
    border: 0px solid rgba(0,64,128,255);
    color: rgba(255,255,255,255);
    margin: 0px;
    background-color: rgba(0,64,128,255);
    selection-background-color: rgba(0,215,255,255);
}

QMenu::separator {
    height: 1px;
    background-color: rgba(0,0,127,255);
}

QMenu::item {
    background-color: rgba(0,64,128,255);
    color: rgba(255,255,255,255);
    padding: 4px 24px 4px 28px;
    /* Reserve space for selection border */
    border: 1px solid transparent;
}

QMenu::item:selected {
    background-color: rgba(0,215,255,255);
    color: rgba(0,0,0,255);
}

QMenu::item:pressed {
    background-color: rgba(0,255,0,255);
    color: rgba(0,0,0,255);
}

QMenu::icon {
    padding-left: 10px;
    width: 14px;
    height: 14px;
}

QMenu::indicator {
    padding-left: 8px;
    width: 12px;
    height: 12px;
}

/* non-exclusive indicator = check box style indicator (see QActionGroup::setExclusive) */
QMenu::indicator:non-exclusive:unchecked {
    image: url('%root%/icons/checkbox_unchecked.png');
}

QMenu::indicator:non-exclusive:unchecked:hover,
QMenu::indicator:non-exclusive:unchecked:focus,
QMenu::indicator:non-exclusive:unchecked:pressed {
    border: none;
    image: url('%root%/icons/checkbox_unchecked_focus.png');
}

QMenu::indicator:non-exclusive:unchecked:disabled {
    image: url('%root%/icons/checkbox_unchecked_disabled.png');
}

QMenu::indicator:non-exclusive:checked {
    image: url('%root%/icons/checkbox_checked.png');
}

QMenu::indicator:non-exclusive:checked:hover,
QMenu::indicator:non-exclusive:checked:focus,
QMenu::indicator:non-exclusive:checked:pressed {
    border: none;
    image: url('%root%/icons/checkbox_checked_focus.png');
}

QMenu::indicator:non-exclusive:checked:disabled {
    image: url('%root%/icons/checkbox_checked_disabled.png');
}

QMenu::indicator:non-exclusive:indeterminate {
    image: url('%root%/icons/checkbox_indeterminate.png');
}

QMenu::indicator:non-exclusive:indeterminate:disabled {
    image: url('%root%/icons/checkbox_indeterminate_disabled.png');
}

QMenu::indicator:non-exclusive:indeterminate:focus,
QMenu::indicator:non-exclusive:indeterminate:hover,
QMenu::indicator:non-exclusive:indeterminate:pressed {
    image: url('%root%/icons/checkbox_indeterminate_focus.png');
}

/* exclusive indicator = radio button style indicator (see QActionGroup::setExclusive) */
QMenu::indicator:exclusive:unchecked {
    image: url('%root%/icons/radio_unchecked.png');
}

QMenu::indicator:exclusive:unchecked:hover,
QMenu::indicator:exclusive:unchecked:focus,
QMenu::indicator:exclusive:unchecked:pressed {
    border: none;
    outline: none;
    image: url('%root%/icons/radio_unchecked_focus.png');
}

QMenu::indicator:exclusive:unchecked:disabled {
    image: url('%root%/icons/radio_unchecked_disabled.png');
}

QMenu::indicator:exclusive:checked {
    border: none;
    outline: none;
    image: url('%root%/icons/radio_checked.png');
}

QMenu::indicator:exclusive:checked:hover,
QMenu::indicator:exclusive:checked:focus,
QMenu::indicator:exclusive:checked:pressed {
    border: none;
    outline: none;
    image: url('%root%/icons/radio_checked_focus.png');
}

QMenu::indicator:exclusive:checked:disabled {
    outline: none;
    image: url('%root%/icons/radio_checked_disabled.png');
}

QMenu::right-arrow {
    margin: 5px;
    padding-left:12px;
    image: url('%root%/icons/arrow_right.png');
    height: 12px;
    width: 12px;
}

/* ##################################################################################### */

/* QAbstractItemView */

QAbstractItemView {
    alternate-background-color: rgba(0,80,140,255);
    color: rgba(255,255,255,255);
    border-color: rgba(0,100,180,255);
}

/* ##################################################################################### */

/* QAbstractScrollArea */

QAbstractScrollArea {
    background-color: rgba(0,64,128,255);
    border-color: rgba(0,100,180,255);
    color: rgba(255,255,255,255);
}

QAbstractScrollArea:disabled {
    color: rgba(127,127,127,255);
}

/* ##################################################################################### */

/* QScrollArea */

QScrollArea QWidget QWidget:disabled {
    background-color: rgba(0,64,128,255);
}

/* ##################################################################################### */

/* QScrollBar */

QScrollBar:horizontal {
    height: 16px;
    margin: 2px 16px 2px 16px;
    border-color: rgba(0,100,180,255);
    border-radius: 4;
    background-color: rgba(0,64,128,255);
}

QScrollBar:vertical {
    background-color: rgba(0,64,128,255);
    width: 16px;
    margin: 16px 2px 16px 2px;
    border-color: rgba(0,100,180,255);
    border-radius: 4;
}

QScrollBar::handle:horizontal {
    background-color: rgba(0,90,156,255);
    border: 1px solid transparent;
    border-radius: 4;
    min-width: 8px;
}

QScrollBar::handle:horizontal:hover {
    background-color: rgba(0,215,255,255);
}

QScrollBar::handle:horizontal:pressed {
    background-color: rgba(0,255,0,255);
}

QScrollBar::handle:vertical {
    background-color: rgba(0,90,156,255);
    min-height: 8px;
    border: 1px solid transparent;
    border-radius: 4;
}

QScrollBar::handle:vertical:hover {
    background-color: rgba(0,215,255,255);
}

QScrollBar::handle:vertical:pressed {
    background-color: rgba(0,255,0,255);
}

QScrollBar::add-line:horizontal {
    margin: 0px 0px 0px 0px;
    border-image: url('%root%/icons/arrow_right_disabled.png');
    height: 12px;
    width: 12px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::add-line:horizontal:hover,
QScrollBar::add-line:horizontal:on {
    border-image: url('%root%/icons/arrow_right.png');
    height: 12px;
    width: 12px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::add-line:vertical {
    margin: 3px 0px 3px 0px;
    border-image: url('%root%/icons/arrow_down_disabled.png');
    height: 12px;
    width: 12px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::add-line:vertical:hover,
QScrollBar::add-line:vertical:on {
    border-image: url('%root%/icons/arrow_down.png');
    height: 12px;
    width: 12px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal {
    margin: 0px 3px 0px 3px;
    border-image: url('%root%/icons/arrow_left_disabled.png');
    height: 12px;
    width: 12px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal:hover,
QScrollBar::sub-line:horizontal:on {
    border-image: url('%root%/icons/arrow_left.png');
    height: 12px;
    width: 12px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical {
    margin: 3px 0px 3px 0px;
    border-image: url('%root%/icons/arrow_up_disabled.png');
    height: 12px;
    width: 12px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical:hover,
QScrollBar::sub-line:vertical:on {
    border-image: url('%root%/icons/arrow_up.png');
    height: 12px;
    width: 12px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

QScrollBar::up-arrow:horizontal,
QScrollBar::down-arrow:horizontal {
    background: none;
}

QScrollBar::up-arrow:vertical,
QScrollBar::down-arrow:vertical {
    background: none;
}

QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal {
    background: none;
}

QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical {
    background: none;
}

/* ##################################################################################### */

/* QTextEdit */

QTextEdit {
    background-color: rgba(0,64,128,255);
    color: rgba(255,255,255,255);
    border: 1px solid rgba(0,100,180,255);
}

QTextEdit:hover {
    border-color: rgba(0,215,255,255);
}

QTextEdit:selected {
    border-color: rgba(0,255,0,255);
}

/* ##################################################################################### */

/* QTextEdit */

QPlainTextEdit {
    background-color: rgba(0,64,128,255);
    color: rgba(255,255,255,255);
    border: 1px solid rgba(0,100,180,255);
}

QPlainTextEdit:focus {
    border-color: rgba(0,255,0,255);
}

QPlainTextEdit:selected {
    background: rgba(0,255,0,255);
    color: rgba(0,0,0,255);
}

/* ##################################################################################### */

/* QSizeGrip */

QSizeGrip {
    background: transparent;
    width: 12px;
    height: 12px;
    image: url('%root%/icons/window_grip.png');
}

/* ##################################################################################### */

/* QStackedWidget */

QStackedWidget {
    padding: 2px;
}

/* ##################################################################################### */

/* QToolBar */

QToolBar {
    background-color: rgba(0,64,128,255);
    border-bottom: 1px solid rgba(0,100,180,255);
    padding: 1px;
    font-weight: bold;
    spacing: 2px;
}

QToolBar:disabled{
    background-color: rgba(0,44,108,255);
}

QToolBar::handle:horizontal {
    width: 16px;
    image: url('%root%/icons/toolbar_move_horizontal.png');
}

QToolBar::handle:vertical {
    height: 16px;
    image: url('%root%/icons/toolbar_move_vertical.png');
}

QToolBar::separator:horizontal {
    width: 16px;
    image: url('%root%/icons/toolbar_separator_horizontal.png');
}

QToolBar::separator:vertical {
    height: 16px;
    image: url('%root%/icons/toolbar_separator_vertical.png');
}

QToolButton#qt_toolbar_ext_button {
    background: rgba(0,64,128,255);
    border: 0px;
    color: rgba(255,255,255,255);
    image: url('%root%/icons/arrow_right.png');
}

/* ##################################################################################### */

/* QAbstractSpinBox */

QAbstractSpinBox {
    background-color: rgba(0,64,128,255);
    border: 1px solid rgba(0,0,127,255);
    color: rgba(255,255,255,255);
    padding-top: 2px;
    padding-bottom: 2px;
    padding-left: 4px;
    padding-right: 4px;
}

QAbstractSpinBox:up-button {
    background-color: transparent rgba(0,64,128,255);
    subcontrol-origin: border;
    subcontrol-position: top right;
    border-left: rgba(0,100,180,255);
    border-bottom: rgba(0,100,180,255);
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
    margin: 1px;
    width: 12px;
    margin-bottom: -1px;
}

QAbstractSpinBox::up-arrow,
QAbstractSpinBox::up-arrow:disabled,
QAbstractSpinBox::up-arrow:off {
    image: url('%root%/icons/arrow_up_disabled.png');
    height: 8px;
    width: 8px;
}

QAbstractSpinBox::up-arrow:hover {
    image: url('%root%/icons/arrow_up.png');
}

QAbstractSpinBox:down-button {
    background-color: transparent rgba(0,64,128,255);
    subcontrol-origin: border;
    subcontrol-position: bottom right;
