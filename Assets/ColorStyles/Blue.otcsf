/* ##################################################################################### */

/* Qt Advanced Docking System */

ads--CDockContainerWidget {
    background: rgba(0,64,128,255);
}

ads--CDockContainerWidget > QSplitter {
    padding: 1 1 1 1;
}

ads--CDockContainerWidget ads--CDockSplitter::handle {
    background: rgba(0,90,156,255);
    border: 1px solid rgba(0,0,127,255);
    /* width: 2; Can't be changed by stylesheet (read notes) */
}

ads--CDockAreaWidget {
    background: rgba(0,64,128,255);
    border: none;
}

#dockAreaTitleBar {
    border-bottom: 1px solid rgba(255,255,255,255);
    bottom: 1px;
}

ads--CDockWidget {
    background: rgba(0,64,128,255);
    border-top: 2px solid transparent;
    border-left: none;
    border-right: none;
    border-bottom: none;
}


QScrollArea#dockWidgetScrollArea {
    padding: 0px;
    border: none;
}

ads--CDockWidgetTab {
    border: 1px solid transparent;
    padding: 0px;
}

ads--CDockWidgetTab[activeTab="false"]::hover {
    border-top: 1px solid rgba(0,215,255,255);
    border-left: 1px solid rgba(0,215,255,255);
    border-right: 1px solid rgba(0,215,255,255);
    border-bottom: 1px solid transparent;
    border-top-left-radius: 4;
    border-top-right-radius: 4;
    margin-bottom: -1px;
}

ads--CDockWidgetTab[activeTab="true"] {
    border-top: 1px solid rgba(255,255,255,255);
    border-left: 1px solid rgba(255,255,255,255);
    border-right: 1px solid rgba(255,255,255,255);
    border-bottom: 1px solid rgba(0,64,128,255);
    border-top-left-radius: 4;
    border-top-right-radius: 4;
    margin-bottom: -1px;
}

ads--CDockWidgetTab QLabel {
    color: rgba(255,255,255,255);
}

ads--CDockWidgetTab[activeTab="true"] QLabel {
    color: rgba(255,255,255,255);
}

#tabCloseButton {
    background: rgba(0,64,128,255);
    color: rgba(255,255,255,255);
    border: 1px solid transparent;
    padding: 0px;
}

#tabCloseButton::hover::!pressed {
    color: rgba(0,215,255,255);
}

#tabCloseButton::pressed {
    color: rgba(0,255,0,255);
}

#tabCloseButton::disabled {
