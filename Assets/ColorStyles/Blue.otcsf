    image: url('%root%/icons/checkbox_unchecked.png');
}

QTreeView::indicator:unchecked:hover,
QTreeView::indicator:unchecked:focus,
QTreeView::indicator:unchecked:pressed,
QListView::indicator:unchecked:hover,
QListView::indicator:unchecked:focus,
QListView::indicator:unchecked:pressed,
QTableView::indicator:unchecked:hover,
QTableView::indicator:unchecked:focus,
QTableView::indicator:unchecked:pressed,
QColumnView::indicator:unchecked:hover,
QColumnView::indicator:unchecked:focus,
QColumnView::indicator:unchecked:pressed {
    image: url('%root%/icons/checkbox_unchecked_focus.png');
}

QTreeView::indicator:indeterminate,
QListView::indicator:indeterminate,
QTableView::indicator:indeterminate,
QColumnView::indicator:indeterminate {
    image: url('%root%/icons/checkbox_indeterminate.png');
}

QTreeView::indicator:indeterminate:hover,
QTreeView::indicator:indeterminate:focus,
QTreeView::indicator:indeterminate:pressed,
QListView::indicator:indeterminate:hover,
QListView::indicator:indeterminate:focus,
QListView::indicator:indeterminate:pressed,
QTableView::indicator:indeterminate:hover,
QTableView::indicator:indeterminate:focus,
QTableView::indicator:indeterminate:pressed,
QColumnView::indicator:indeterminate:hover,
QColumnView::indicator:indeterminate:focus,
QColumnView::indicator:indeterminate:pressed {
    image: url('%root%/icons/checkbox_indeterminate_focus.png');
}

QTreeView,
QListView,
QTableView,
QColumnView {
    background-color: rgba(0,64,128,255);
    color: rgba(255,255,255,255);
    gridline-color: rgba(0,100,180,255);
    margin: 0;
    padding: 0;
}

QTreeView:disabled,
QListView:disabled,
QTableView:disabled,
QColumnView:disabled {
    background-color: rgba(0,44,108,255);
    color: rgba(127,127,127,255);
}

QTreeView:selected,
QListView:selected,
QTableView:selected,
QColumnView:selected {
    border-color: rgba(0,255,0,255);
}

QTreeView:focus,
QListView:focus,
QTableView:focus,
QColumnView:focus {
    border-color: rgba(0,255,0,255);
}

QTreeView::item:pressed,
QListView::item:pressed,
QTableView::item:pressed,
QColumnView::item:pressed {
    background-color: rgba(0,255,0,255);
    color: rgba(0,0,0,255);
}

QTreeView::item:selected:active,
QListView::item:selected:active,
QTableView::item:selected:active,
QColumnView::item:selected:active {
    background-color: rgba(0,255,0,255);   
    color: rgba(0,0,0,255);
}

QTreeView::item:selected:!active,
QListView::item:selected:!active,
QTableView::item:selected:!active,
QColumnView::item:selected:!active {
    background-color: rgba(0,255,0,255);
    color: rgba(0,0,0,255);
}

QTreeView::item:!selected:hover,
QListView::item:!selected:hover,
QTableView::item:!selected:hover,
QColumnView::item:!selected:hover {
    outline: 0;
    color: rgba(0,0,0,255);
    background-color: rgba(0,215,255,255);
}

QTableCornerButton::section {
    background-color: rgba(0,90,156,255);
    border: 1px solid transparent;
    border-radius: 0px;
}

/* ##################################################################################### */

/* QToolBox */

QToolBox {
    padding: 0px;
    border: 0px;
    border-color: rgba(0,100,180,255);
}

QToolBox:selected {
    padding: 0px;
    border: 2px solid rgba(0,255,0,255);
}

QToolBox::tab {
    background-color: rgba(0,64,128,255);
    border-color: rgba(0,100,180,255);
    color: rgba(255,255,255,255);
    border-top-left-radius: 4;
    border-top-right-radius: 4;
}

QToolBox::tab:disabled {
    color: rgba(127,127,127,255);
}

QToolBox::tab:selected {
    background-color: rgba(0,64,128,255);
    border-bottom: 2px solid rgba(0,100,180,255);
}

QToolBox::tab:selected:disabled {
    background-color: rgba(0,64,128,255);
    border-bottom: 2px solid rgba(127,127,127,255);
}

QToolBox::tab:!selected {
    background-color: rgba(0,64,128,255);
    border-bottom: 2px solid rgba(0,64,128,255);
}

QToolBox::tab:!selected:disabled {
    background-color: rgba(0,44,108,255);
}

QToolBox::tab:hover {
    border-color: rgba(0,215,255,255);
    border-bottom: 2px solid rgba(0,215,255,255);
}

QToolBox QScrollArea {
    padding: 0px;
    border: 0px;
    background-color: rgba(0,64,128,255);
}

/* ##################################################################################### */

/* QFrame */

.QFrame {
    border-color: rgba(0,100,180,255);
    border-width: 0;
}

/* No frame */
.QFrame[frameShape="0"] {
    border: 1px transparent rgba(0,64,128,255);
}

/* HLine */
.QFrame[frameShape="4"] {
    max-height: 2px;
    border: none;
    background-color: rgba(0,64,128,255);
}

/* HLine */
.QFrame[frameShape="5"] {
    max-width: 2px;
    border: none;
    background-color: rgba(0,64,128,255);
}

/* ##################################################################################### */

/* QSplitter */

QSplitter {
    background-color: rgba(0,64,128,255);
    spacing: 0px;
    padding: 0px;
    margin: 0px;
    border: 0px solid transparent;
}

QSplitter::handle {
    background-color: rgba(0,90,156,255);
    border: 1px solid rgba(0,0,127,255);
    border-radius: 2;
    spacing: 1px;
    padding: 1px;
}

QSplitter::handle:hover:!pressed {
    background-color: rgba(0,215,255,255);
    border: 1px solid rgba(0,0,127,255);
    border-radius: 2;
}

QSplitter::handle:pressed {
    background-color: rgba(0,64,128,255);
    border: 1px solid rgba(0,255,0,255);
    border-radius: 2;
}

/* ##################################################################################### */

/* QDateEdit, QDateTimeEdit */

QDateEdit, QDateTimeEdit {
    selection-background-color: rgba(0,255,0,255);
    border-style: solid;
    border-color: rgba(0,100,180,255);
    border-width: 0;  

    /* This fixes 103, 111 */
    padding-top: 2px;

    /* This fixes 103, 111 */
    padding-bottom: 2px;
    padding-left: 4px;
    padding-right: 4px;
    min-width: 10px;
}

QDateEdit:on,
QDateTimeEdit:on {
    selection-background-color: rgba(0,255,0,255);
}

QDateEdit::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 12px;
    border-left: 1px solid rgba(0,100,180,255);
}

QDateEdit::down-arrow,
QDateTimeEdit::down-arrow {
    image: url('%root%/icons/arrow_down_disabled.png');
    height: 8px;
    width: 8px;
}

QDateEdit::down-arrow:on,
QDateEdit::down-arrow:hover,
QDateEdit::down-arrow:focus,
QDateTimeEdit::down-arrow:on,
QDateTimeEdit::down-arrow:hover,
QDateTimeEdit::down-arrow:focus {
    image: url('%root%/icons/arrow_down.png');
}

QDateEdit QAbstractItemView,
QDateTimeEdit QAbstractItemView {
    background-color: rgba(0,64,128,255);
    border-radius: 4;
    border-color: rgba(0,100,180,255);
    border-width: 0;
    selection-background-color: rgba(0,255,0,255);
}


/* ##################################################################################### */

/* QColorDialog */

QColorDialog {
    background-color: rgba(0,90,156,255);
    color: rgba(255,255,255,255);
}

/* ##################################################################################### */

/* QAbstractView */

QAbstractView:hover {
    border-color: rgba(0,215,255,255);
}

QAbstractView:selected {
    background: rgba(0,255,0,255);
    color: rgba(0,0,0,255);
}

