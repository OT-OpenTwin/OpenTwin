    padding-bottom: 4px;
    text-align: left;
}

ot--ComboButton:!hover:!pressed {
    background-color: rgba(255,255,255,255);
    color: rgba(0,0,0,255);
    border: 1px solid rgba(168,168,168,255);
    padding-top: 4px;
    padding-bottom: 4px;
    text-align: left;
}

ot--ComboButton:hover:!pressed {
    background-color: rgba(255,255,255,255);
    color: rgba(0,0,0,255);
    border: 1px solid rgba(0,215,255,255);
    padding-top: 4px;
    padding-bottom: 4px;
    text-align: left;
}

ot--ComboButton[IsSelected="true"] {
    background-color: rgba(255,255,255,255);
    color: rgba(0,0,0,255);
    border: 1px solid rgba(0,255,0,255);
    padding-top: 4px;
    padding-bottom: 4px;
    text-align: left;
}

ot--ComboButton[IsSelected="false"]:!hover:pressed {
    background-color: rgba(255,255,255,255);
