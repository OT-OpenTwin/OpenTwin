    image: url('%root%/icons/log_in_checkbox_indeterminate_disabled.png');
}

LogInDialog #LogInDialogSavePassword::indicator:indeterminate:focus,
LogInDialog #LogInDialogSavePassword::indicator:indeterminate:hover {
    border: none;
    image: url('%root%/icons/log_in_checkbox_indeterminate_focus.png');
}

LogInDialog #LogInDialogSavePassword::indicator:indeterminate:pressed {
    border: none;
    image: url('%root%/icons/log_in_checkbox_indeterminate_pressed.png');
}


