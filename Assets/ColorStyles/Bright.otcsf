/* ##################################################################################### */

/* IndicatorWidget */

ot--IndicatorWidget {
    color: rgba(0,0,0,255);
    background: rgba(0,0,0,0);
    border: 1px solid rgba(0,0,0,255);
    border-radius: 4;
}

ot--IndicatorWidget:hover {
    color: rgba(0,215,255,255);
    border-color: rgba(0,215,255,255);
}

ot--IndicatorWidget:pressed {
    color: rgba(0,255,0,255);
    border-color: rgba(0,255,0,255);
}

ot--IndicatorWidget:disabled {
    color: rgba(168,168,168,255);
    border-color: rgba(128,128,128,255);
}

/* ##################################################################################### */

/* Special Properties */

QWidget[OT_TransparentBackground="true"]:!hover:!pressed {
    background: transparent;
}

QWidget[OT_TransparentHoverBackground="true"]:hover:!pressed {
    background: transparent;
}

QWidget[OT_TransparentPressedBackground="true"]:pressed {
    background: transparent;
}

QWidget[OT_Borderless="true"] {
    border: 1px solid transparent;
}

QWidget[SuccessForeground="true"] {
    color: rgba(0,255,0,255);
}

QWidget[WarningForeground="true"] {
    color: rgba(255,165,0,255);
}

QWidget[ErrorForeground="true"] {
    color: rgba(255,0,0,255);
}
