/* ##################################################################################### */

/* TabToolBar */

#OT_TTB {
    border: 1px solid transparent;
}

#OT_TTB QTabBar::tab::first,
#OT_TTB QTabBar::tab::only-one {
    background-color: rgba(0,215,255,255);
    color: rgba(0,0,0,255);
    border-bottom: 1px solid rgba(0,0,0,255);
    border-top-left-radius: 4;
    border-top-right-radius: 4;
}

#OT_TTB QTabBar::tab::first:selected,
#OT_TTB QTabBar::tab::only-one:selected {
    background-color: qlineargradient(x1: 0.5, y1: 0, x2: 0.5, y2: 1, stop: 0 rgba(0,215,255,255), stop: 0.3 rgba(0,215,255,255), stop: 1 rgba(255,255,255,255));
}

#OT_TTBGroup {
    border-right: 1px solid rgba(0,0,0,255);
    margin-bottom: 5px;
}

/* ##################################################################################### */

/* Qt Advanced Docking System */

ads--CDockContainerWidget {
    background: rgba(255,255,255,255);
}

ads--CDockContainerWidget > QSplitter {
    padding: 1 1 1 1;
}

ads--CDockContainerWidget ads--CDockSplitter::handle {
    background: rgba(211,211,211,255);
    border: 1px solid rgba(168,168,168,255);
    /* width: 2; Can't be changed by stylesheet (read notes) */
}

ads--CDockAreaWidget {
    background: rgba(255,255,255,255);
    border: none;
}

#dockAreaTitleBar {
    border-bottom: 1px solid rgba(0,0,0,255);
    bottom: 1px;
}

ads--CDockWidget {
    background: rgba(255,255,255,255);
    border-top: 2px solid transparent;
    border-left: none;
    border-right: none;
    border-bottom: none;
}


QScrollArea#dockWidgetScrollArea {
    padding: 0px;
    border: none;
}

ads--CDockWidgetTab {
    border: 1px solid transparent;
    padding: 2px;
}

ads--CDockWidgetTab[activeTab="false"]::hover {
    border-top: 1px solid rgba(0,215,255,255);
    border-left: 1px solid rgba(0,215,255,255);
    border-right: 1px solid rgba(0,215,255,255);
    border-bottom: 1px solid transparent;
    border-top-left-radius: 4;
    border-top-right-radius: 4;
    margin-bottom: -2px;
}

ads--CDockWidgetTab[activeTab="true"] {
    border-top: 1px solid rgba(0,0,0,255);
    border-left: 1px solid rgba(0,0,0,255);
    border-right: 1px solid rgba(0,0,0,255);
    border-bottom: 1px solid rgba(255,255,255,255);
    border-top-left-radius: 4;
    border-top-right-radius: 4;
    margin-bottom: -2px;
}

ads--CDockWidgetTab QLabel {
    color: rgba(0,0,0,255);
    padding: 0px;
    margin: 0px;
}

ads--CDockWidgetTab[activeTab="true"] QLabel {
    color: rgba(0,0,0,255);
}

ads--CDockWidgetTab #tabCloseButton {
    background: rgba(255,255,255,255);
    color: rgba(0,0,0,255);
    border: 0px solid transparent;
    padding: 0px;
    margin: 0px;
    width: 8px;
    image: url('%root%/icons/window_close.png');
}

ads--CDockWidgetTab #tabCloseButton::hover::!pressed {
    image: url('%root%/icons/window_close_focus.png');
}

ads--CDockWidgetTab #tabCloseButton::pressed {
    image: url('%root%/icons/window_close_pressed.png');
}

ads--CDockWidgetTab #tabCloseButton::disabled {
    image: url('%root%/icons/window_close_disabled.png');
}

#tabsMenuButton::menu-indicator {
        image: none;
}

#tabsMenuButton {
        qproperty-icon: url('%root%/icons/arrow_down.png'),
                url('%root%/icons/arrow_down_disabled.png') disabled;
        qproperty-iconSize: 10px;
}

#dockAreaCloseButton {
        qproperty-icon: url('%root%/icons/window_close.png'),
                url('%root%/icons/window_close_disabled.png') disabled;
        qproperty-iconSize: 10px;
}

#detachGroupButton {
        qproperty-icon: url('%root%/icons/window_undock.png'),
                url('%root%/icons/window_undock_disabled.png') disabled;
        qproperty-iconSize: 10px;
}

