sheet:
/*    OpenTwin Notes                                                                     */
/*                                                                                       */
/*    Splitter Handle:                                                                   */
/*       When changing the width the qt-ads (third party) library must be adjusted       */
/*       (see notes file) and recompiled.                                                */
/*                                                                                       */
/* This file used the "QDarkStyleSheet/qdarkstyle/qss/_style.scss" as a template.        */
/*                                                                                       */

/* Reset */

* {
    padding: 0px;
    margin: 0px;
    border: 0px;
    border-style: none;
    border-image: none;
    outline: 0;
}

QToolBar * {
    margin: 0px;
    padding: 0px;
}

/* ##################################################################################### */

/* QWidget */

QWidget {
    background-color: rgba(255,255,255,255);
    border: none;
    padding: 0px;
    color: rgba(0,0,0,255);
    selection-background-color: rgba(0,255,0,255);
    selection-color: rgba(0,0,0,255);
}

QWidget:disabled {
    background-color: rgba(230,230,230,255);
    color: rgba(168,168,168,255);
    selection-background-color: rgba(230,230,230,255);
    selection-color: rgba(168,168,168,255);
}

QWidget::item {
    background-color: rgba(255,255,255,255);
}

QWidget::item:selected {
    background-color: rgba(0,255,0,255);
    color: rgba(0,0,0,255);
}

QWidget::item:hover {
    background-color: rgba(0,215,255,255);
}

/* ##################################################################################### */

/* QMainWindow */

QMainWindow {
    background-color: rgba(255,255,255,255);
}

QMainWindow::separator {
    background-color: rgba(211,211,211,255);
    spacing: 0px;
    padding: 2px;
}

QMainWindow::separator:hover:!pressed {
    background-color: rgba(0,215,255,255);
}

QMainWindow::separator:pressed {
    background-color: rgba(0,255,0,255);
}

QMainWindow::separator:horizontal {
    width: 2;
    margin-top: 2px;
    margin-bottom: 2px;
}

QMainWindow::separator:vertical {
    height: 2;
    margin-left: 2px;
    margin-right: 2px;
}

/* ##################################################################################### */

/* QToolTip */

QToolTip {
    background-color: rgba(255,255,255,255);
    color: rgba(0,0,0,255);
    border: 1px solid rgba(0,0,0,255);
    /* Remove padding, for fix combo box tooltip */
    padding: 0px;
}

/* ##################################################################################### */

/* QStatusBar */

QStatusBar {
    background: rgba(255,255,255,255);
    border-top: 1px solid rgba(0,0,0,255);
}

QStatusBar::item {
    border: 1px solid transparent;
}

QStatusBar QLabel {
    background: transparent;
}

/* ##################################################################################### */

/* QCheckBox */

QCheckBox {
    background: rgba(255,255,255,255);
    color: rgba(0,0,0,255);
    spacing: 4px;
    outline: none;
    padding-top: 4px;
    padding-bottom: 4px;
}
    
QCheckBox:focus {
    border: none;
}

QCheckBox:disabled {
    background: transparent;
    color: rgba(168,168,168,255);
}

QCheckBox::indicator {
    background: transparent;
    margin-left: 2px;
    height: 14px;
    width: 14px;
}
 
QCheckBox::indicator:unchecked {
    image: url('%root%/icons/checkbox_unchecked.png');
}

QCheckBox::indicator:unchecked:hover,
QCheckBox::indicator:unchecked:focus {
    border: none;
    image: url('%root%/icons/checkbox_unchecked_focus.png');
}

QCheckBox::indicator:unchecked:pressed {
    border: none;
    image: url('%root%/icons/checkbox_unchecked_pressed.png');
}

QCheckBox::indicator:unchecked:disabled {
    image: url('%root%/icons/checkbox_unchecked_disabled.png');
}

QCheckBox::indicator:checked {
    image: url('%root%/icons/checkbox_checked.png');
}

QCheckBox::indicator:checked:hover,
QCheckBox::indicator:checked:focus {
    border: none;
    image: url('%root%/icons/checkbox_checked_focus.png');
}

QCheckBox::indicator:checked:pressed {
    border: none;
    image: url('%root%/icons/checkbox_checked_pressed.png');
}

QCheckBox::indicator:checked:disabled {
    image: url('%root%/icons/checkbox_checked_disabled.png');
}

QCheckBox::indicator:indeterminate {
    image: url('%root%/icons/checkbox_indeterminate.png');
}

QCheckBox::indicator:indeterminate:disabled {
    image: url('%root%/icons/checkbox_indeterminate_disabled.png');
}

QCheckBox::indicator:indeterminate:focus,
QCheckBox::indicator:indeterminate:hover {
    image: url('%root%/icons/checkbox_indeterminate_focus.png');
}

QCheckBox::indicator:indeterminate:pressed {
    image: url('%root%/icons/checkbox_indeterminate_pressed.png');
}

/* ##################################################################################### */

/* QGroupBox */

QGroupBox {
    font-weight: bold;
    border: 1px solid rgba(0,0,0,255);
    border-radius: 4;
    padding: 2px;
    margin-top: 6px;
    margin-bottom: 4px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    left: 4px;
    padding-left: 2px;
    padding-right: 4px;
    padding-top: -4px;
}

QGroupBox::indicator {
    margin-left: 2px;
    margin-top: 2px;
    padding: 0;
    height: 14px;
    width: 14px;
}

QGroupBox::indicator:unchecked {
    border: none;
    image: url('%root%/icons/checkbox_unchecked.png');
}

QGroupBox::indicator:unchecked:hover,
QGroupBox::indicator:unchecked:focus,
QGroupBox::indicator:unchecked:pressed {
    border: none;
    image: url('%root%/icons/checkbox_unchecked_focus.png');
}

QGroupBox::indicator:unchecked:disabled {
    image: url('%root%/icons/checkbox_unchecked_disabled.png');
}

QGroupBox::indicator:checked {
    border: none;
    image: url('%root%/icons/checkbox_checked.png');
}

QGroupBox::indicator:checked:hover,
QGroupBox::indicator:checked:focus,
QGroupBox::indicator:checked:pressed {
    border: none;
    image: url('%root%/icons/checkbox_checked_focus.png');
}

QGroupBox::indicator:checked:disabled {
    image: url('%root%/icons/checkbox_checked_disabled.png');
}

/* ##################################################################################### */

/* QRadioButton */

QRadioButton {
    background-color: rgba(255,255,255,255);
    color: rgba(0,0,0,255);
    spacing: 4px;
    padding-top: 4px;
    padding-bottom: 4px;
    border: none;
    outline: none;
}

QRadioButton:focus {
    border: none;
}

QRadioButton:disabled {
    background-color: rgba(255,255,255,255);
    color: rgba(168,168,168,255);
    border: none;
    outline: none;
}

QRadioButton QWidget {
    background-color: rgba(255,255,255,255);
    color: rgba(0,0,0,255);
    spacing: 0px;
    padding: 0px;
    outline: none;
    border: none;
}

QRadioButton::indicator {
    border: none;
    outline: none;
    margin-left: 2px;
    height: 14px;
    width: 14px;
}

QRadioButton::indicator:unchecked {
    image: url('%root%/icons/radio_unchecked.png');
}

QRadioButton::indicator:unchecked:hover,
QRadioButton::indicator:unchecked:focus,
QRadioButton::indicator:unchecked:pressed {
    border: none;
    outline: none;
    image: url('%root%/icons/radio_unchecked_focus.png');
}

QRadioButton::indicator:disabled {
    image: url('%root%/icons/radio_unchecked_disabled.png');
}
        
QRadioButton::indicator:disabled:checked {
    border: none;
    outline: none;
    image: url('%root%/icons/radio_checked.png');
}

QRadioButton::indicator:disabled:checked:hover,
QRadioButton::indicator:disabled:checked:focus,
QRadioButton::indicator:disabled:checked:pressed {
    border: none;
    outline: none;
    image: url('%root%/icons/radio_checked_focus.png');
}

QRadioButton::indicator:disabled:checked:disabled {
    outline: none;
    image: url('%root%/icons/radio_checked_disabled.png');
}

/* ##################################################################################### */

/* QMenuBar */

QMenuBar {
    background-color: rgba(255,255,255,255);
    padding: 2px;
    border-bottom: 1px solid rgba(0,0,0,255);
    margin-bottom: 2px;
    color: rgba(0,0,0,255);
    selection-background-color: rgba(0,255,0,255);
    selection-color: rgba(0,0,0,255);
}

QMenuBar:focus {
    border-color: rgba(0,215,255,255);
}

QMenuBar::item {
    background: transparent;
    padding: 4px;
}

QMenuBar::item:selected {
    padding: 4px;
    border: 0px solid rgba(0,0,0,255);
    background-color: rgba(0,215,255,255);
    color: rgba(0,0,0,255);
}

QMenuBar::item:pressed {
    padding: 4px;
    border: 0px solid rgba(255,255,255,255);
    background-color: rgba(0,255,0,255);
    color: rgba(0,0,0,255);
    margin-bottom: 0px;
