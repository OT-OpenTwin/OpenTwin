    max-height: 2px;
    border: none;
    background-color: %color:Widget Background%;
}

/* HLine */
.QFrame[frameShape="5"] {
    max-width: 2px;
    border: none;
    background-color: %color:Widget Background%;
}

/* ##################################################################################### */

/* QSplitter */

QSplitter {
    background-color: %color:Window Background%;
    spacing: 0px;
    padding: 0px;
    margin: 0px;
    border: 0px solid transparent;
}

QSplitter::handle {
    background-color: %color:Input Background%;
    border: 1px solid %color:Border Light Color%;
    border-radius: %int:Splitter Border Radius%;
    spacing: 1px;
    padding: 1px;
}

QSplitter::handle:hover:!pressed {
    background-color: %color:Widget Hover Background%;
    border: 1px solid %color:Border Light Color%;
    border-radius: %int:Splitter Border Radius%;
}

QSplitter::handle:pressed {
    background-color: %color:Widget Background%;
    border: 1px solid %color:Border Selection Color%;
    border-radius: %int:Splitter Border Radius%;
}

/* ##################################################################################### */

/* QDateEdit, QDateTimeEdit */

QDateEdit, QDateTimeEdit {
    selection-background-color: %color:Widget Selection Background%;
    border-style: solid;
    border-color: %color:Border Color%;
    border-width: %double:Border Width%;  

    /* This fixes 103, 111 */
    padding-top: 2px;

    /* This fixes 103, 111 */
    padding-bottom: 2px;
    padding-left: 4px;
    padding-right: 4px;
    min-width: 10px;
}

QDateEdit:on,
QDateTimeEdit:on {
    selection-background-color: %color:Widget Selection Background%;
}

QDateEdit::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 12px;
    border-left: 1px solid %color:Border Color%;
}

QDateEdit::down-arrow,
QDateTimeEdit::down-arrow {
    image: url('%file:Arrow Down Disabled Icon%');
    height: 8px;
    width: 8px;
}

QDateEdit::down-arrow:on,
QDateEdit::down-arrow:hover,
QDateEdit::down-arrow:focus,
QDateTimeEdit::down-arrow:on,
QDateTimeEdit::down-arrow:hover,
QDateTimeEdit::down-arrow:focus {
    image: url('%file:Arrow Down Icon%');
}

QDateEdit QAbstractItemView,
QDateTimeEdit QAbstractItemView {
    background-color: %color:Widget Background%;
    border-radius: %double:Border Radius Big%;
    border-color: %color:Border Color%;
    border-width: %double:Border Width%;
    selection-background-color: %color:Widget Selection Background%;
}


/* ##################################################################################### */

/* QColorDialog */

QColorDialog {
    background-color: %color:Dialog Background%;
    color: %color:Dialog Foreground%;
}

/* ##################################################################################### */

/* Property Grid */

ot--PropertyGridTree::item {
    background-color: transparent;
    color: %color:Widget Foreground%;
}

ot--PropertyGridTree::item:hover:!selected {
    background-color: transparent;
    color: %color:Widget Foreground%;
}

ot--PropertyGridTree::item:selected {
    background-color: transparent;
    color: %color:Widget Foreground%;
}

/* ##################################################################################### */

/* Log In */

LogInDialog {
    background: rgb(2, 41, 92);
    border: 1px solid white;
}

LogInDialog QLabel {
    color: white;
}

LogInDialog #LogInDialogRegisterLabel:hover {
    color: %color:Widget Hover Background%;
}

LogInDialog #LogInDialogChangePasswordLabel:hover {
    color: %color:Widget Hover Background%;
}

LogInDialog #LogInDialogImageView {
    background: rgb(2, 41, 92);
}

LogInDialog #LogInDialogSessionServiceUrl {
    border-radius: 0;
    text-align: left;
}

LogInDialog #LogInDialogSavePassword {
    background-color: transparent;
    color: white;
    spacing: 4px;
    outline: none;
    padding-top: 4px;
    padding-bottom: 4px;
}
    
LogInDialog #LogInDialogSavePassword:focus {
    border: none;
}

LogInDialog #LogInDialogSavePassword:disabled {
    background-color: transparent;
    color: lightgray;
}

LogInDialog #LogInDialogSavePassword::indicator {
    margin-left: 2px;
    height: 14px;
    width: 14px;
}
 
LogInDialog #LogInDialogSavePassword::indicator:unchecked {
    image: url('%file:LogIn CheckBox Unchecked Icon%');
}

LogInDialog #LogInDialogSavePassword::indicator:unchecked:hover,
LogInDialog #LogInDialogSavePassword::indicator:unchecked:focus {
    border: none;
    image: url('%file:LogIn CheckBox Unchecked Focus Icon%');
}

LogInDialog #LogInDialogSavePassword::indicator:unchecked:pressed {
    border: none;
    image: url('%file:LogIn CheckBox Unchecked Pressed Icon%');
}

LogInDialog #LogInDialogSavePassword::indicator:unchecked:disabled {
    image: url('%file:LogIn CheckBox Unchecked Disabled Icon%');
}

LogInDialog #LogInDialogSavePassword::indicator:checked {
    image: url('%file:LogIn CheckBox Checked Icon%');
}

LogInDialog #LogInDialogSavePassword::indicator:checked:hover,
LogInDialog #LogInDialogSavePassword::indicator:checked:focus {
    border: none;
    image: url('%file:LogIn CheckBox Checked Focus Icon%');
}

LogInDialog #LogInDialogSavePassword::indicator:checked:pressed {
    border: none;
    image: url('%file:LogIn CheckBox Checked Pressed Icon%');
}

LogInDialog #LogInDialogSavePassword::indicator:checked:disabled {
    border: none;
    image: url('%file:LogIn CheckBox Checked Disabled Icon%');
}

LogInDialog #LogInDialogSavePassword::indicator:indeterminate {
    border: none;
    image: url('%file:LogIn CheckBox Indeterminate Icon%');
}

LogInDialog #LogInDialogSavePassword::indicator:indeterminate:disabled {
    border: none;
    image: url('%file:LogIn CheckBox Indeterminate Disabled Icon%');
}

LogInDialog #LogInDialogSavePassword::indicator:indeterminate:focus,
LogInDialog #LogInDialogSavePassword::indicator:indeterminate:hover {
    border: none;
    image: url('%file:LogIn CheckBox Indeterminate Focus Icon%');
}

LogInDialog #LogInDialogSavePassword::indicator:indeterminate:pressed {
    border: none;
    image: url('%file:LogIn CheckBox Indeterminate Pressed Icon%');
}


/* ##################################################################################### */

/* Line Edit */

ot--LineEdit {
    border-radius: 0;
}

/* ##################################################################################### */

/* Combo Button */

ot--ComboButton {
    background-color: %color:Widget Background%;
    color: %color:Widget Foreground%;
    border: 1px solid %color:Border Light Color%;
    padding-top: 4px;
    padding-bottom: 4px;
    text-align: left;
}

ot--ComboButton:!hover:!pressed {
    background-color: %color:Widget Background%;
    color: %color:Widget Foreground%;
    border: 1px solid %color:Border Light Color%;
    padding-top: 4px;
    padding-bottom: 4px;
    text-align: left;
}

ot--ComboButton:hover:!pressed {
    background-color: %color:Widget Background%;
    color: %color:Widget Foreground%;
    border: 1px solid %color:Border Hover Color%;
    padding-top: 4px;
    padding-bottom: 4px;
    text-align: left;
}

ot--ComboButton[IsSelected="true"] {
    background-color: %color:Widget Background%;
    color: %color:Widget Foreground%;
    border: 1px solid %color:Border Selection Color%;
    padding-top: 4px;
    padding-bottom: 4px;
    text-align: left;
}

ot--ComboButton[IsSelected="false"]:!hover:pressed {
    background-color: %color:Widget Background%;
    color: %color:Widget Foreground%;
    border: 1px solid %color:Border Light Color%;
    padding-top: 4px;
    padding-bottom: 4px;
    text-align: left;
}

ot--ComboButton[IsSelected="false"]:hover:pressed {
    background-color: %color:Widget Background%;
    color: %color:Widget Foreground%;
    border: 1px solid %color:Border Hover Color%;
    padding-top: 4px;
    padding-bottom: 4px;
    text-align: left;
}

/* ##################################################################################### */

/* Text Edit Search Popup */

ot--TextEditorSearchPopup {
    background-color: %color:Title Background%;
    padding: 4px;
    border: 1px solid %color:Border Color%;
}

/* ##################################################################################### */

/* Property Input Color */

ot--ColorPickButton #OT_PropertyInputColor_Button {
    background-color: %color:Widget Background%;
    color: %color:Widget Foreground%;
    border: 1px solid %color:Border Color%;
}

ot--ColorPickButton #OT_PropertyInputColor_Button:hover:!pressed {
    background-color: %color:Widget Background%;
    color: %color:Widget Foreground%;
    border: 1px solid %color:Border Hover Color%;
}

ot--ColorPickButton #OT_PropertyInputColor_Button:pressed {
    background-color: %color:Widget Background%;
    color: %color:Widget Foreground%;
    border: 1px solid %color:Border Selection Color%;
}

/* ##################################################################################### */

/* Property Input Painter2D */

ot--Painter2DEditButton #OT_PropertyInputPainter2D_Button {
    background-color: %color:Widget Background%;
    color: %color:Widget Foreground%;
    border: 1px solid %color:Border Color%;
}

ot--Painter2DEditButton #OT_PropertyInputPainter2D_Button:hover:!pressed {
    background-color: %color:Widget Background%;
    color: %color:Widget Foreground%;
    border: 1px solid %color:Border Hover Color%;
}

ot--Painter2DEditButton #OT_PropertyInputPainter2D_Button:pressed {
    background-color: %color:Widget Background%;
    color: %color:Widget Foreground%;
    border: 1px solid %color:Border Selection Color%;
}

/* ##################################################################################### */

/* TabToolBar */

#OT_TTB {
    border: 1px solid transparent;
}

#OT_TTB QTabBar::tab::first,
#OT_TTB QTabBar::tab::only-one {
    background-color: %color:TabToolBar First Tab Background%;
    color: %color:TabToolBar First Tab Foreground%;
    border-bottom: 1px solid %color:Border Color%;
    border-top-left-radius: %double:Border Radius Big%;
    border-top-right-radius: %double:Border Radius Big%;
}

#OT_TTB QTabBar::tab::first:selected,
#OT_TTB QTabBar::tab::only-one:selected {
    background-color: qlineargradient(x1: 0.5, y1: 0, x2: 0.5, y2: 1, stop: 0 %color:TabToolBar First Tab Background%, stop: 0.3 %color:TabToolBar First Tab Background%, stop: 1 %color:Widget Background%);
}

#OT_TTBGroup {
    border-right: 1px solid %color:Border Color%;
    margin-bottom: 5px;
}

/* ##################################################################################### */

/* Qt Advanced Docking System */

ads--CDockContainerWidget {
    background: %color:Window Background%;
}

ads--CDockContainerWidget > QSplitter {
    padding: 1 1 1 1;
}

ads--CDockContainerWidget ads--CDockSplitter::handle {
    background: %color:Input Background%;
    border: 1px solid %color:Border Light Color%;
    /* width: %int:Splitter Handle Width%; Can't be changed by stylesheet (read notes) */
}

ads--CDockAreaWidget {
    background: %color:Widget Background%;
    border: none;
}

#dockAreaTitleBar {
    border-bottom: 1px solid %color:Title Border%;
    bottom: 1px;
}

ads--CDockWidget {
    background: %color:Widget Background%;
    border-top: 2px solid transparent;
    border-left: none;
    border-right: none;
    border-bottom: none;
}


QScrollArea#dockWidgetScrollArea {
    padding: 0px;
    border: none;
}

ads--CDockWidgetTab {
    border: 1px solid transparent;
    padding: 2px;
}

ads--CDockWidgetTab[activeTab="false"]::hover {
    border-top: 1px solid %color:Border Hover Color%;
    border-left: 1px solid %color:Border Hover Color%;
    border-right: 1px solid %color:Border Hover Color%;
    border-bottom: 1px solid transparent;
    border-top-left-radius: %double:Border Radius Big%;
    border-top-right-radius: %double:Border Radius Big%;
    margin-bottom: -2px;
}

ads--CDockWidgetTab[activeTab="true"] {
    border-top: 1px solid %color:Title Border%;
    border-left: 1px solid %color:Title Border%;
    border-right: 1px solid %color:Title Border%;
    border-bottom: 1px solid %color:Widget Background%;
    border-top-left-radius: %double:Border Radius Big%;
    border-top-right-radius: %double:Border Radius Big%;
    margin-bottom: -2px;
}

ads--CDockWidgetTab QLabel {
    color: %color:Widget Foreground%;
    padding: 0px;
    margin: 0px;
}

ads--CDockWidgetTab[activeTab="true"] QLabel {
    color: %color:Widget Foreground%;
}

ads--CDockWidgetTab #tabCloseButton {
    background: %color:Widget Background%;
    color: %color:Widget Foreground%;
    border: 0px solid transparent;
    padding: 0px;
    margin: 0px;
    width: 8px;
    image: url('%file:Window Close Icon%');
}

ads--CDockWidgetTab #tabCloseButton::hover::!pressed {
    image: url('%file:Window Close Focus Icon%');
}

ads--CDockWidgetTab #tabCloseButton::pressed {
    image: url('%file:Window Close Pressed Icon%');
}

ads--CDockWidgetTab #tabCloseButton::disabled {
    image: url('%file:Window Close Disabled Icon%');
}

#tabsMenuButton::menu-indicator {
        image: none;
}

#tabsMenuButton {
        qproperty-icon: url('%file:Arrow Down Icon%'),
                url('%file:Arrow Down Disabled Icon%') disabled;
        qproperty-iconSize: 10px;
}

#dockAreaCloseButton {
        qproperty-icon: url('%file:Window Close Icon%'),
                url('%file:Window Close Disabled Icon%') disabled;
        qproperty-iconSize: 10px;
}

#detachGroupButton {
        qproperty-icon: url('%file:Window Undock Icon%'),
                url('%file:Window Undock Disabled Icon%') disabled;
        qproperty-iconSize: 10px;
}

/* ##################################################################################### */

/* View Buttons */

#OT_ViewTabCloseButton {
    background: transparent;
    border: none;
    image: url('%file:Window Close Icon%');
}

#OT_ViewTabCloseButton:hover:!pressed {
    image: url('%file:Window Close Focus Icon%');
}

#OT_ViewTabCloseButton:pressed {
    image: url('%file:Window Close Pressed Icon%');
}

#OT_ViewTabCloseButton:disabled {
    image: url('%file:Window Close Disabled Icon%');
}

#OT_ViewTabPinButton[IsPinned="true"] {
    background: transparent;
    border: none;
    image: url('%file:Window Pin Icon%');
}

#OT_ViewTabPinButton[IsPinned="true"]:hover:!pressed {
    image: url('%file:Window Pin Focus Icon%');
}

#OT_ViewTabPinButton[IsPinned="true"]:pressed {
    image: url('%file:Window Pin Pressed Icon%');
}

#OT_ViewTabPinButton[IsPinned="true"]:disabled {
    image: url('%file:Window Pin Disabled Icon%');
}

#OT_ViewTabPinButton[IsPinned="false"] {
    background: transparent;
    border: none;
    image: url('%file:Window Unpin Icon%');
}

#OT_ViewTabPinButton[IsPinned="false"]:hover:!pressed {
    image: url('%file:Window Unpin Focus Icon%');
}

#OT_ViewTabPinButton[IsPinned="false"]:pressed {
    image: url('%file:Window Unpin Pressed Icon%');
}

#OT_ViewTabPinButton[IsPinned="false"]:disabled {
    image: url('%file:Window Unpin Disabled Icon%');
}

/* ##################################################################################### */

/* Special Properties */

QWidget[OT_TransparentBackground="true"]:!hover:!pressed {
    background: transparent;
}

QWidget[OT_TransparentHoverBackground="true"]:hover:!pressed {
    background: transparent;
}

QWidget[OT_TransparentPressedBackground="true"]:pressed {
    background: transparent;
}

QWidget[OT_Borderless="true"] {
