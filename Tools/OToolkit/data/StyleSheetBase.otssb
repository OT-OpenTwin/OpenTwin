/*    OpenTwin Notes                                                                     */
/*                                                                                       */
/*    Splitter Handle:                                                                   */
/*       When changing the width the qt-ads (third party) library must be adjusted       */
/*       (see notes file) and recompiled.                                                */
/*                                                                                       */
/* This file used the "QDarkStyleSheet/qdarkstyle/qss/_style.scss" as a template.        */
/*                                                                                       */

/* Reset */

* {
    padding: 0px;
    margin: 0px;
    border: 0px;
    border-style: none;
    border-image: none;
    outline: 0;
}

QToolBar * {
    margin: 0px;
    padding: 0px;
}

/* ##################################################################################### */

/* QWidget */

QWidget {
    background-color: %color:Widget Background%;
    border: none;
    padding: 0px;
    color: %color:Widget Foreground%;
    selection-background-color: %color:Widget Selection Background%;
    selection-color: %color:Widget Selection Foreground%;
}

QWidget:disabled {
    background-color: %color:Widget Disabled Background%;
    color: %color:Widget Disabled Foreground%;
    selection-background-color: %color:Widget Disabled Background%;
    selection-color: %color:Widget Disabled Foreground%;
}

QWidget::item {
    background-color: %color:Widget Background%;
}

QWidget::item:selected {
    background-color: %color:Widget Selection Background%;
    color: %color:Widget Selection Foreground%;
}

QWidget::item:hover {
    background-color: %color:Widget Hover Background%;
}

/* ##################################################################################### */

/* QMainWindow */

QMainWindow {
    background-color: %color:Window Background%;
}

QMainWindow::separator {
    background-color: %color:Input Background%;
    spacing: 0px;
    padding: 2px;
}

QMainWindow::separator:hover:!pressed {
    background-color: %color:Widget Hover Background%;
}

QMainWindow::separator:pressed {
    background-color: %color:Widget Selection Background%;
}

QMainWindow::separator:horizontal {
    width: %int:Splitter Handle Width%;
    margin-top: 2px;
    margin-bottom: 2px;
}

QMainWindow::separator:vertical {
    height: %int:Splitter Handle Width%;
    margin-left: 2px;
    margin-right: 2px;
}

/* ##################################################################################### */

/* QToolTip */

QToolTip {
    background-color: %color:Widget Background%;
    color: %color:Widget Foreground%;
    border: 1px solid %color:Border Color%;
    /* Remove padding, for fix combo box tooltip */
    padding: 0px;
}

/* ##################################################################################### */

/* QStatusBar */

QStatusBar {
    background: %color:Widget Background%;
    border-top: 1px solid %color:Border Color%;
}

QStatusBar::item {
    border: 1px solid transparent;
}

QStatusBar QLabel {
    background: transparent;
}

/* ##################################################################################### */

/* QCheckBox */

QCheckBox {
    background: %color:Widget Background%;
    color: %color:Widget Foreground%;
    spacing: 4px;
    outline: none;
    padding-top: 4px;
    padding-bottom: 4px;
}
    
QCheckBox:focus {
    border: none;
}

QCheckBox:disabled {
    background: transparent;
    color: %color:Widget Disabled Foreground%;
}

QCheckBox::indicator {
    background: transparent;
    margin-left: 2px;
    height: 14px;
    width: 14px;
}
 
QCheckBox::indicator:unchecked {
    image: url('%file:CheckBox Unchecked Icon%');
}

QCheckBox::indicator:unchecked:hover,
QCheckBox::indicator:unchecked:focus {
    border: none;
    image: url('%file:CheckBox Unchecked Focus Icon%');
}

QCheckBox::indicator:unchecked:pressed {
    border: none;
    image: url('%file:CheckBox Unchecked Pressed Icon%');
}

QCheckBox::indicator:unchecked:disabled {
    image: url('%file:CheckBox Unchecked Disabled Icon%');
}

QCheckBox::indicator:checked {
    image: url('%file:CheckBox Checked Icon%');
}

QCheckBox::indicator:checked:hover,
QCheckBox::indicator:checked:focus {
    border: none;
    image: url('%file:CheckBox Checked Focus Icon%');
}

QCheckBox::indicator:checked:pressed {
    border: none;
    image: url('%file:CheckBox Checked Pressed Icon%');
}

QCheckBox::indicator:checked:disabled {
    image: url('%file:CheckBox Checked Disabled Icon%');
}

QCheckBox::indicator:indeterminate {
    image: url('%file:CheckBox Indeterminate Icon%');
}

QCheckBox::indicator:indeterminate:disabled {
    image: url('%file:CheckBox Indeterminate Disabled Icon%');
}

QCheckBox::indicator:indeterminate:focus,
QCheckBox::indicator:indeterminate:hover {
    image: url('%file:CheckBox Indeterminate Focus Icon%');
}

QCheckBox::indicator:indeterminate:pressed {
    image: url('%file:CheckBox Indeterminate Pressed Icon%');
}

/* ##################################################################################### */

/* QGroupBox */

QGroupBox {
    font-weight: bold;
    border: 1px solid %color:Border Color%;
    border-radius: %double:Border Radius Big%;
    padding: 2px;
    margin-top: 6px;
    margin-bottom: 4px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    left: 4px;
    padding-left: 2px;
    padding-right: 4px;
    padding-top: -4px;
}

QGroupBox::indicator {
    margin-left: 2px;
    margin-top: 2px;
    padding: 0;
    height: 14px;
    width: 14px;
}

QGroupBox::indicator:unchecked {
    border: none;
    image: url('%file:CheckBox Unchecked Icon%');
}

QGroupBox::indicator:unchecked:hover,
QGroupBox::indicator:unchecked:focus,
QGroupBox::indicator:unchecked:pressed {
    border: none;
    image: url('%file:CheckBox Unchecked Focus Icon%');
}

QGroupBox::indicator:unchecked:disabled {
    image: url('%file:CheckBox Unchecked Disabled Icon%');
}

QGroupBox::indicator:checked {
    border: none;
    image: url('%file:CheckBox Checked Icon%');
}

QGroupBox::indicator:checked:hover,
QGroupBox::indicator:checked:focus,
QGroupBox::indicator:checked:pressed {
    border: none;
    image: url('%file:CheckBox Checked Focus Icon%');
}

QGroupBox::indicator:checked:disabled {
    image: url('%file:CheckBox Checked Disabled Icon%');
}

/* ##################################################################################### */

/* QRadioButton */

QRadioButton {
    background-color: %color:Widget Background%;
    color: %color:Widget Foreground%;
    spacing: 4px;
    padding-top: 4px;
    padding-bottom: 4px;
    border: none;
    outline: none;
}

QRadioButton:focus {
    border: none;
}

QRadioButton:disabled {
    background-color: %color:Widget Background%;
    color: %color:Widget Disabled Foreground%;
    border: none;
    outline: none;
}

QRadioButton QWidget {
    background-color: %color:Widget Background%;
    color: %color:Widget Foreground%;
    spacing: 0px;
    padding: 0px;
    outline: none;
    border: none;
}

QRadioButton::indicator {
    border: none;
    outline: none;
    margin-left: 2px;
    height: 14px;
    width: 14px;
}

QRadioButton::indicator:unchecked {
    image: url('%file:RadioButton Unchecked Icon%');
}

QRadioButton::indicator:unchecked:hover,
QRadioButton::indicator:unchecked:focus,
QRadioButton::indicator:unchecked:pressed {
    border: none;
    outline: none;
    image: url('%file:RadioButton Unchecked Focus Icon%');
}

QRadioButton::indicator:disabled {
    image: url('%file:RadioButton Unchecked Disabled Icon%');
}
        
QRadioButton::indicator:disabled:checked {
    border: none;
    outline: none;
    image: url('%file:RadioButton Checked Icon%');
}

QRadioButton::indicator:disabled:checked:hover,
QRadioButton::indicator:disabled:checked:focus,
QRadioButton::indicator:disabled:checked:pressed {
    border: none;
    outline: none;
    image: url('%file:RadioButton Checked Focus Icon%');
}

QRadioButton::indicator:disabled:checked:disabled {
    outline: none;
    image: url('%file:RadioButton Checked Disabled Icon%');
}

/* ##################################################################################### */

/* QMenuBar */

QMenuBar {
    background-color: %color:Header Background%;
    padding: 2px;
    border-bottom: 1px solid %color:Border Color%;
    margin-bottom: 2px;
    color: %color:Widget Foreground%;
    selection-background-color: %color:Widget Selection Background%;
    selection-color: %color:Widget Selection Foreground%;
}

QMenuBar:focus {
    border-color: %color:Border Hover Color%;
}

QMenuBar::item {
    background: transparent;
    padding: 4px;
}

QMenuBar::item:selected {
    padding: 4px;
    border: 0px solid %color:Border Color%;
    background-color: %color:Widget Hover Background%;
    color: %color:Widget Hover Foreground%;
}

QMenuBar::item:pressed {
    padding: 4px;
    border: 0px solid %color:Widget Background%;
    background-color: %color:Widget Selection Background%;
    color: %color:Widget Selection Foreground%;
    margin-bottom: 0px;
    padding-bottom: 0px;
}

/* ##################################################################################### */

/* QMenu */

QMenu {
    border: 1px solid %color:Border Light Color%;
    color: %color:Widget Foreground%;
    margin: 0px;
    background-color: %color:Widget Background%;
    selection-background-color: %color:Widget Hover Background%;
}

QMenu::separator {
    height: 1px;
    background-color: %color:Border Light Color%;
}

QMenu::item {
    background-color: %color:Widget Background%;
    color: %color:Widget Foreground%;
    padding: 4px 24px 4px 28px;
    /* Reserve space for selection border */
    border: 1px solid transparent;
}

QMenu::item:selected {
    background-color: %color:Widget Hover Background%;
    color: %color:Widget Hover Foreground%;
}

QMenu::item:pressed {
    background-color: %color:Widget Selection Background%;
    color: %color:Widget Selection Foreground%;
}

QMenu::icon {
    padding-left: 10px;
    width: 14px;
    height: 14px;
}

QMenu::indicator {
    padding-left: 8px;
    width: 12px;
    height: 12px;
}

/* non-exclusive indicator = check box style indicator (see QActionGroup::setExclusive) */
QMenu::indicator:non-exclusive:unchecked {
    image: url('%file:CheckBox Unchecked Icon%');
}

QMenu::indicator:non-exclusive:unchecked:hover,
QMenu::indicator:non-exclusive:unchecked:focus,
QMenu::indicator:non-exclusive:unchecked:pressed {
    border: none;
    image: url('%file:CheckBox Unchecked Focus Icon%');
}

QMenu::indicator:non-exclusive:unchecked:disabled {
    image: url('%file:CheckBox Unchecked Disabled Icon%');
}

QMenu::indicator:non-exclusive:checked {
    image: url('%file:CheckBox Checked Icon%');
}

QMenu::indicator:non-exclusive:checked:hover,
QMenu::indicator:non-exclusive:checked:focus,
QMenu::indicator:non-exclusive:checked:pressed {
    border: none;
    image: url('%file:CheckBox Checked Focus Icon%');
}

QMenu::indicator:non-exclusive:checked:disabled {
    image: url('%file:CheckBox Checked Disabled Icon%');
}

QMenu::indicator:non-exclusive:indeterminate {
    image: url('%file:CheckBox Indeterminate Icon%');
}

QMenu::indicator:non-exclusive:indeterminate:disabled {
    image: url('%file:CheckBox Indeterminate Disabled Icon%');
}

QMenu::indicator:non-exclusive:indeterminate:focus,
QMenu::indicator:non-exclusive:indeterminate:hover,
QMenu::indicator:non-exclusive:indeterminate:pressed {
    image: url('%file:CheckBox Indeterminate Focus Icon%');
}

/* exclusive indicator = radio button style indicator (see QActionGroup::setExclusive) */
QMenu::indicator:exclusive:unchecked {
    image: url('%file:RadioButton Unchecked Icon%');
}

QMenu::indicator:exclusive:unchecked:hover,
QMenu::indicator:exclusive:unchecked:focus,
QMenu::indicator:exclusive:unchecked:pressed {
    border: none;
    outline: none;
    image: url('%file:RadioButton Unchecked Focus Icon%');
}

QMenu::indicator:exclusive:unchecked:disabled {
    image: url('%file:RadioButton Unchecked Disabled Icon%');
}

QMenu::indicator:exclusive:checked {
    border: none;
    outline: none;
    image: url('%file:RadioButton Checked Icon%');
}

QMenu::indicator:exclusive:checked:hover,
QMenu::indicator:exclusive:checked:focus,
QMenu::indicator:exclusive:checked:pressed {
    border: none;
    outline: none;
    image: url('%file:RadioButton Checked Focus Icon%');
}

QMenu::indicator:exclusive:checked:disabled {
    outline: none;
    image: url('%file:RadioButton Checked Disabled Icon%');
}

QMenu::right-arrow {
    margin: 5px;
    padding-left:12px;
    image: url('%file:Arrow Right Icon%');
    height: 12px;
    width: 12px;
}

/* ##################################################################################### */

/* QAbstractItemView */

QAbstractItemView {
    alternate-background-color: %color:Widget Alternate Background%;
    color: %color:Widget Foreground%;
    border-color: %color:Border Color%;
}

/* ##################################################################################### */

/* QAbstractScrollArea */

QAbstractScrollArea {
    background-color: %color:Widget Background%;
    border-color: %color:Border Color%;
    color: %color:Widget Foreground%;
}

QAbstractScrollArea:disabled {
    color: %color:Widget Disabled Foreground%;
}

/* ##################################################################################### */

/* QScrollArea */

QScrollArea QWidget QWidget:disabled {
    background-color: %color:Widget Background%;
}

/* ##################################################################################### */

/* QScrollBar */

QScrollBar:horizontal {
    height: 16px;
    margin: 2px 16px 2px 16px;
    border-color: %color:Border Color%;
    border-radius: %double:Border Radius Big%;
    background-color: %color:Widget Background%;
}

QScrollBar:vertical {
    background-color: %color:Widget Background%;
    width: 16px;
    margin: 16px 2px 16px 2px;
    border-color: %color:Border Color%;
    border-radius: %double:Border Radius Big%;
}

QScrollBar::handle:horizontal {
    background-color: %color:Input Background%;
    border: 1px solid transparent;
    border-radius: %double:Border Radius Small%;
    min-width: 8px;
}

QScrollBar::handle:horizontal:hover {
    background-color: %color:Widget Hover Background%;
}

QScrollBar::handle:horizontal:pressed {
    background-color: %color:Widget Selection Background%;
}

QScrollBar::handle:vertical {
    background-color: %color:Input Background%;
    min-height: 8px;
    border: 1px solid transparent;
    border-radius: %double:Border Radius Small%;
}

QScrollBar::handle:vertical:hover {
    background-color: %color:Widget Hover Background%;
}

QScrollBar::handle:vertical:pressed {
    background-color: %color:Widget Selection Background%;
}

QScrollBar::add-line:horizontal {
    margin: 0px 0px 0px 0px;
    border-image: url('%file:Arrow Right Disabled Icon%');
    height: 12px;
    width: 12px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::add-line:horizontal:hover,
QScrollBar::add-line:horizontal:on {
    border-image: url('%file:Arrow Right Icon%');
    height: 12px;
    width: 12px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::add-line:vertical {
    margin: 3px 0px 3px 0px;
    border-image: url('%file:Arrow Down Disabled Icon%');
    height: 12px;
    width: 12px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::add-line:vertical:hover,
QScrollBar::add-line:vertical:on {
    border-image: url('%file:Arrow Down Icon%');
    height: 12px;
    width: 12px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal {
    margin: 0px 3px 0px 3px;
    border-image: url('%file:Arrow Left Disabled Icon%');
    height: 12px;
    width: 12px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal:hover,
QScrollBar::sub-line:horizontal:on {
    border-image: url('%file:Arrow Left Icon%');
    height: 12px;
    width: 12px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical {
    margin: 3px 0px 3px 0px;
    border-image: url('%file:Arrow Up Disabled Icon%');
    height: 12px;
    width: 12px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical:hover,
QScrollBar::sub-line:vertical:on {
    border-image: url('%file:Arrow Up Icon%');
    height: 12px;
    width: 12px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

QScrollBar::up-arrow:horizontal,
QScrollBar::down-arrow:horizontal {
    background: none;
}

QScrollBar::up-arrow:vertical,
QScrollBar::down-arrow:vertical {
    background: none;
}

QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal {
    background: none;
}

QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical {
    background: none;
}

/* ##################################################################################### */

/* QTextEdit */

QTextEdit {
    background-color: %color:Widget Background%;
    color: %color:Widget Foreground%;
    border: 1px solid %color:Border Color%;
}

QTextEdit:hover {
    border-color: %color:Border Hover Color%;
}

QTextEdit:selected {
    border-color: %color:Border Selection Color%;
}

/* ##################################################################################### */

/* QTextEdit */

QPlainTextEdit {
    background-color: %color:Widget Background%;
    color: %color:Widget Foreground%;
    border: 1px solid %color:Border Color%;
}

QPlainTextEdit:focus {
    border-color: %color:Border Selection Color%;
}

QPlainTextEdit:selected {
    background: %color:Widget Selection Background%;
    color: %color:Widget Selection Foreground%;
}

/* ##################################################################################### */

/* QSizeGrip */

QSizeGrip {
    background: transparent;
    width: 12px;
    height: 12px;
    image: url('%file:Window Grip Icon%');
}

/* ##################################################################################### */

/* QStackedWidget */

QStackedWidget {
    padding: 2px;
}

/* ##################################################################################### */

/* QToolBar */

QToolBar {
    background-color: %color:Widget Background%;
    border-bottom: 1px solid %color:Border Color%;
    padding: 1px;
    font-weight: bold;
    spacing: 2px;
}

QToolBar:disabled{
    background-color: %color:Widget Disabled Background%;
}

QToolBar::handle:horizontal {
    width: 16px;
    image: url('%file:ToolBar Move Horizontal Icon%');
}

QToolBar::handle:vertical {
    height: 16px;
    image: url('%file:ToolBar Move Vertical Icon%');
}

QToolBar::separator:horizontal {
    width: 16px;
    image: url('%file:ToolBar Separator Horizontal Icon%');
}

QToolBar::separator:vertical {
    height: 16px;
    image: url('%file:ToolBar Separator Vertical Icon%');
}

QToolButton#qt_toolbar_ext_button {
    background: %color:Widget Background%;
    border: 0px;
    color: %color:Widget Foreground%;
    image: url('%file:Arrow Right Icon%');
}

/* ##################################################################################### */

/* QAbstractSpinBox */

QAbstractSpinBox {
    background-color: %color:Widget Background%;
    border: 1px solid %color:Border Light Color%;
    color: %color:Widget Foreground%;
    padding-top: 2px;
    padding-bottom: 2px;
    padding-left: 4px;
    padding-right: 4px;
}

QAbstractSpinBox:up-button {
    background-color: transparent %color:Widget Background%;
    subcontrol-origin: border;
    subcontrol-position: top right;
    border-left: %color:Border Color%;
    border-bottom: %color:Border Color%;
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
    margin: 1px;
    width: 12px;
    margin-bottom: -1px;
}

QAbstractSpinBox::up-arrow,
QAbstractSpinBox::up-arrow:disabled,
QAbstractSpinBox::up-arrow:off {
    image: url('%file:Arrow Up Disabled Icon%');
    height: 8px;
    width: 8px;
}

QAbstractSpinBox::up-arrow:hover {
    image: url('%file:Arrow Up Icon%');
}

QAbstractSpinBox:down-button {
    background-color: transparent %color:Widget Background%;
    subcontrol-origin: border;
    subcontrol-position: bottom right;
    border-left: %color:Border Color%;
    border-top: %color:Border Color%;
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
    margin: 1px;
    width: 12px;
    margin-top: -1px;
}

QAbstractSpinBox::down-arrow,
QAbstractSpinBox::down-arrow:disabled,
QAbstractSpinBox::down-arrow:off {
    image: url('%file:Arrow Down Disabled Icon%');
    height: 8px;
    width: 8px;
}

QAbstractSpinBox::down-arrow:hover {
    image: url('%file:Arrow Down Icon%');
}

QAbstractSpinBox:hover {
    border-color: %color:Border Hover Color%;
    color: %color:Widget Foreground%;
}

QAbstractSpinBox:focus {
    border-color: %color:Border Selection Color%;
}

QAbstractSpinBox:selected {
    background: %color:Widget Selection Background%;
    color: %color:Widget Selection Foreground%;
}

/* ##################################################################################### */

/* QLabel */

QLabel {
    background-color: transparent;
    border: 0px solid transparent;
    padding: 2px;
    margin: 0px;
    color: %color:Widget Foreground%;
}

QLabel:disabled {
    background-color: transparent;
    color: %color:Widget Disabled Foreground%;
}

/* ##################################################################################### */

/* QTextBrowser */

QTextBrowser {
    background-color: %color:Widget Background%;
    color: %color:Widget Foreground%;
    border: 1px solid %color:Border Light Color%;
}

QTextBrowser:disabled {
    background-color: %color:Widget Background%;
    border-color: %color:Border Color%;
    color: %color:Widget Disabled Foreground%;
}

QTextBrowser:hover {
    border-color: %color:Border Hover Color%;
}

QTextBrowser:selected {
    border-color: %color:Border Selection Color%;
}

/* ##################################################################################### */

/* QGraphicsView */

QGraphicsView {
    background-color: %color:Widget Background%;
    border-color: %color:Border Color%;
    color: %color:Widget Foreground%;
}

QGraphicsView:disabled {
    background-color: %color:Widget Background%;
    border-color: %color:Border Color%;
    color: %color:Widget Disabled Foreground%;
}

QGraphicsView:hover,
QGraphicsView:!hover,
QGraphicsView:selected,
QGraphicsView:pressed {
    border-color: %color:Border Color%;
}

/* ##################################################################################### */

/* QCalendarWidget */

QCalendarWidget {
    border-color: %color:Border Color%;
}

QCalendarWidget:disabled {
        background-color: %color:Widget Disabled Background%;
        color: %color:Widget Disabled Foreground%;
}

/* ##################################################################################### */

/* QLCDNumber */

QLCDNumber {
    background-color: %color:Widget Background%;
    color: %color:Widget Foreground%;
}

QLCDNumber:disabled {
    background-color: %color:Widget Background%;
    color: %color:Widget Disabled Foreground%;
}

/* ##################################################################################### */

/* QProgressBar */

QProgressBar {
    background-color: %color:Widget Background%;
    border-color: %color:Border Color%;
    color: %color:Widget Foreground%;
    border-radius: %double:Border Radius Big%;
    text-align: center;
}

QProgressBar:disabled {
    background-color: %color:Widget Background%;
    border-color: %color:Border Color%;
    color: %color:Widget Disabled Foreground%;
    border-radius: %double:Border Radius Big%;
    text-align: center;
}

QProgressBar::chunk {
    background-color: %color:Widget Selection Background%;
    color: %color:Widget Foreground%;
    border-radius: %double:Border Radius Big%;
}

QProgressBar::chunk:disabled {
    background-color: %color:Widget Disabled Background%;
    color: %color:Widget Disabled Foreground%;
    border-radius: %double:Border Radius Big%;
}

/* ##################################################################################### */

/* QPushButton */

QPushButton {
    background-color: %color:Input Background%;
    color: %color:Widget Foreground%;
    padding-top: 2px;
    padding-bottom: 2px;
    padding-left: 6px;
    padding-right: 6px;
    outline: none;
    border: 1px solid %color:Border Color%;
    border-style: outset;
    border-radius: %double:Border Radius Big%;
}

QPushButton:disabled {
    background-color: %color:Input Background%;
    color: %color:Widget Disabled Foreground%;
    border-radius: %double:Border Radius Big%;
}

QPushButton:checked {
    background-color: %color:Widget Background%;
    outline: none;
}

QPushButton:checked:disabled {
    background-color: %color:Widget Background%;
    color: %color:Widget Disabled Foreground%;
    border-radius: %double:Border Radius Big%;
    outline: none;
}

QPushButton:hover:!pressed {
    background-color: %color:Widget Hover Background%;
    color: %color:Widget Hover Foreground%;
}

QPushButton:pressed {
